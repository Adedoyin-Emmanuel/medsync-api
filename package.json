{
  "name": "nodejs-template",
  "version": "1.0.0",
  "description": "A simple nodejs template for building nodejs applications",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "check-lint": "eslint [0-9]*.ts",
    "build": "rimraf ./dist && tsc index.ts --esModuleInterop true --target ES6 --module nodenext --moduleResolution nodenext --outDir dist",
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon index.ts",
    "test": "jest",
    "full-test": "eslint */[0-9]*.ts && jest --watchAll --verbose",
    "full-test-fix": "eslint */[0-9]*.ts --fix && jest --watchAll --verbose",
    "studio": "bunx prisma studio",
    "migrate": "bunx prisma migrate dev",
    "generate": "bunx prisma generate"
  },
  "proxy": true,
  "keywords": [],
  "author": "Adedoyin Emmanuel Adeniyi",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.8.0",
    "@babel/preset-env": "^7.6.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/config": "^3.3.1",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-slow-down": "^1.3.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.199",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.7.1",
    "@types/nodemailer": "^6.4.11",
    "@types/uuid": "^9.0.5",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": "18.18.0"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@squadco/js": "^1.3.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.2.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.0.2",
    "helmet": "^7.1.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.6",
    "redoc": "^2.1.4",
    "socket.io": "^4.7.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  }
}
